<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="pinyougou-shop-web"/>
    <dubbo:annotation package="com.pinyougou"/>
    <dubbo:registry id="zookeeper" protocol="zookeeper" address="192.168.1.159:2181"/>
    <dubbo:protocol id="dubbo" name="dubbo" port="20881"/>
    <dubbo:reference id="sellerService" interface="com.pinyougou.service.SellerService" version="1.0" registry="zookeeper" protocol="dubbo"/>
    <dubbo:reference id="goodsService" interface="com.pinyougou.service.GoodsService" version="1.0" registry="zookeeper" protocol="dubbo"/>
    <dubbo:reference id="uploadFileService" interface="com.pinyougou.service.UploadFileService" version="1.0" registry="zookeeper" protocol="dubbo"/>
    <dubbo:reference id="itemCatService" interface="com.pinyougou.service.ItemCatService" version="1.0" registry="zookeeper" protocol="dubbo"/>
    <dubbo:reference id="brandService" interface="com.pinyougou.service.BrandService" version="1.0" registry="zookeeper" protocol="dubbo"/>
    <dubbo:reference id="typeTemplateService" interface="com.pinyougou.service.TypeTemplateService" version="1.0" registry="zookeeper" protocol="dubbo"/>
</beans>


